{
  "name": "Pretty OkCupid",
  "short_name": "Pretty_Okc",
  "version": "0.1",
  "manifest_version": 2,
  "description": "UX / Styling improvements for OkCupid.",
  "homepage_url": "http://www.mpelletier.net",
  "content_scripts": [
    {
      "matches": ["http://www.okcupid.com/match*", "http://www.okcupid.com/profile/*"],
      "css": ["style.css"],
      "js": [
        "jquery-1.10.2.min.js",
        "mutation-summary.min.js",
        "init.js"
        ],
      "run_at": "document_end"
    }
  ]
}
